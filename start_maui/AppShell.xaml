<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="start_maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:start_maui"
    xmlns:pagea="clr-namespace:start_maui.pagea"
    Title="start_maui">

    <!-- <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:new_page}"
        Route="new_page"/> -->

    <TabBar>
        <Tab Title="Reminders">
            <ShellContent ContentTemplate="{DataTemplate pagea:Main_page}"></ShellContent>
        </Tab>
        <Tab Title="Friends">
            <ShellContent ContentTemplate="{DataTemplate local:Friends}"></ShellContent>
        </Tab>
        <Tab Title="Settings">
            <ShellContent ContentTemplate="{DataTemplate local:Settings}"></ShellContent>
        </Tab>
    </TabBar>

</Shell>
